citHeader("To cite Thermimage in publications, including its version and DOI, please use:")

year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype="Misc",
    title = "Thermimage: Thermal Image Analysis",
    author = person( "Tattersall, GJ"),
    year = year,
    note = note,
    url = "https://CRAN.R-project.org/package=Thermimage",
    doi = "10.5281/zenodo.1069704"
    )
